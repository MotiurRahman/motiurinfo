                              <button type="button" class="btn btn-primary btn-lg" id="sendMes"><i
                                      class="fa fa-spinner fa-spin"></i> Message Sending</button>
                              <button type="button" class="btn btn-success btn-lg" id="submitButton">Send</button>
                          </div>
                      </div>
                  </form>
              </div>
          </div>
      </div>
